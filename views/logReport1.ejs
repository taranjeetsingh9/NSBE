                    </g>
                    <defs>
                        <clipPath id="clip0_17_1425">
                            <rect width="24" height="24" fill="white" transform="translate(0.5)" />
                        </clipPath>
                    </defs>
                </svg>
                <li>Settings</li>
            </div>

        </ul>
    </footer>
    <script>
        // Get all elements with class 'svg-container'
        const svgContainers = document.querySelectorAll('.svg-container');

        // Add click event listener to each div
        svgContainers.forEach(function (container) {
            const svgPath = container.querySelector('.svgPath');

            container.addEventListener('click', function () {
                // Change SVG fill color to red for the clicked div
                svgPath.classList.toggle('selected');


            });
        });
    </script>
</body>

</html>